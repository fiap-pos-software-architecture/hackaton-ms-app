{
   "name": "hackaton-ms-app",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
     "start": "node dist/src/index",
     "predev": "npm run swagger",
     "prebuild": "rimraf ./dist && yarn swagger",
     "dev": "nodemon --exec ts-node ./src/index.ts",
     "dev-tsoa": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
     "swagger": "tsoa spec",
     "migration:generate": "typeorm-ts-node-commonjs -d src/data-source.ts migration:generate ./src/migrations/default",
     "migration:run": "typeorm-ts-node-commonjs -d src/data-source.ts migration:run",
     "build": "yarn prebuild && tsc",
     "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
     "format": "prettier --write \"src/**/*.ts\"",
     "type:check": "yarn swagger && yarn run tsc",
     "docker:up": "rimraf dist && docker-compose up -d",
     "docker:down": "docker-compose down",
     "test": "jest",
     "coverage": "jest --coverage",
     "frame:generate": "ffmpeg -i src/fast-food-project.mp4 -r 0.05 outputFile_%02d.png"
   },
   "dependencies": {
     "@ffmpeg-installer/ffmpeg": "^1.1.0",
     "@types/joi": "^17.2.3",
     "body-parser": "^1.20.2",
     "cors": "~2.8.5",
     "dotenv": "^16.4.7",
     "express": "^4.21.2",
     "ffmpeg": "^0.0.4",
     "ffmpeg.js": "^4.2.9003",
     "fluent-ffmpeg": "^2.1.3",
     "http-status": "^1.7.4",
     "jsonwebtoken": "^9.0.2",
     "moment": "^2.30.1",
     "morgan": "~1.10.0",
     "multer": "^1.4.5-lts.1",
     "mysql2": "^3.9.7",
     "nodemon": "^3.1.0",
     "reflect-metadata": "^0.2.2",
     "swagger-ui-express": "^5.0.0",
     "typeorm": "^0.3.20"
   },
   "devDependencies": {
     "@eslint/js": "^9.11.1",
     "@types/bcrypt": "^5.0.2",
     "@types/body-parser": "^1.19.5",
     "@types/cors": "^2.8.17",
     "@types/express": "^4.17.21",
     "@types/fluent-ffmpeg": "^2.1.27",
     "@types/jest": "^29.5.13",
     "@types/jsonwebtoken": "^9.0.8",
     "@types/morgan": "^1.9.9",
     "@types/multer": "^1.4.12",
     "@types/node": "^20.17.17",
     "@types/swagger-ui-express": "^4.1.6",
     "globals": "^15.9.0",
     "jest": "^29.7.0",
     "prettier": "~3.2.5",
     "prettier-plugin-organize-imports": "~3.2.4",
     "rimraf": "~5.0.7",
     "sonar-scanner": "^3.1.0",
     "ts-jest": "^29.2.5",
     "ts-node": "^10.9.2",
     "tsconfig-paths": "^4.2.0",
     "tsoa": "^6.2.1",
     "typescript": "^5.7.3",
     "typescript-eslint": "^8.7.0"
   }
 }